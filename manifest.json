{
    "manifest_version": 3,
    "name": "Plus New Tab",
    "description": "A customizable new tab page.",
    "version": "0.1",
    "permissions": [
        "management",
        "favicon"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "options_ui": {
        "page": "options.html"
    },
    "icons": {
        "128": "icon.png"
    }
}