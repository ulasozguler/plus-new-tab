{
	"manifest_version": 2,
	"name": "Special New Tab",
	"description": "A customized new tab page.",
	"version": "0.1",
	"permissions": ["management"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
