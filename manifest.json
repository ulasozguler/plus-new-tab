{
  "manifest_version": 2,
  "name": "Plus New Tab",
  "description": "A customizable new tab page.",
  "version": "0.1",
  "permissions": [
    "management"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "128": "icon.png"
  }
}
